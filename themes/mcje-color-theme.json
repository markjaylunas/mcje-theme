{
	"$schema": "vscode://schemas/color-theme",
	"name": "mcje",
	"tokenColors": [
	  {
		"name": "Global settings",
		"settings": {
		//   "background": "#09131b",
		  "foreground": "#FFFFFF"
		}
	  },
	  {
		"name": "String",
		"scope": "string",
		"settings": {
		  "foreground": "#C8F2EF"
		}
	  },
	  {
		"name": "String Escape",
		"scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
		"settings": {
		  "foreground": "#1efaa2"
		}
	  },
	  {
		"name": "Boolean",
		"scope": "constant.language.boolean",
		"settings": {
		  "foreground": "#4BE3AC"
		}
	  },
	  {
		"name": "Number",
		"scope": "constant.numeric",
		"settings": {
		  "foreground": "#FFE400"
		}
	  },
	  {
		"name": "Identifier",
		"scope": "variable, support.variable, support.class, support.constant, meta.definition.variable, entity.name.function",
		"settings": {
		  "foreground": "#FAC213"
		}
	  },
	  {
		"name": "Constant",
		"scope": "variable.other.constant",
		"settings": {
		  "foreground": "#ff5177"
		}
	  },
	  {
		"name": "Keyword",
		"scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
		"settings": {
		  "foreground": "#34B3F1"
		}
	  },
	  {
		"name": "Keyword",
		"scope": "keyword.control.flow.js",
		"settings": {
		  "foreground": "#FFE400"
		}
	  },
	  {
		"name": "Function call",
		"scope": "entity.name.function, support.function",
		"settings": {
		  "foreground": "#FFF89A"
		}
	  },
	  {
		"name": "Storage",
		"scope": "storage.type, storage.modifier, keyword.operator.expression",
		"settings": {
		  "foreground": "#4BE3AC"
		}
	  },
	  {
		"name": "Modules",
		"scope": "support.module, support.node",
		"settings": {
		  "foreground": "#ff9746",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Type",
		"scope": "support.type",
		"settings": {
		  "foreground": "#FFEFCF"
		}
	  },
	  {
		"name": "Type",
		"scope": "entity.name.type, entity.other.inherited-class",
		"settings": {
		  "foreground": "#ff9d00"
		}
	  },
	  {
		"name": "Comment",
		"scope": "comment, comment.block.js",
		"settings": {
		  "foreground": "#577997",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Class",
		"scope": "entity.name.type.class",
		"settings": {
		  "foreground": "#ff9d00",
		  "fontStyle": "underline"
		}
	  },
	  {
		"name": "Class variable",
		"scope": "variable.object.property, meta.field.declaration entity.name.function",
		"settings": {
		  "foreground": "#34B3F1"
		}
	  },
	  {
		"name": "Class method",
		"scope": "meta.definition.method entity.name.function",
		"settings": {
		  "foreground": "#F47340"
		}
	  },
	  {
		"name": "Function definition",
		"scope": "meta.function, entity.name.function",
		"settings": {
		  "foreground": "#FE7E6D"
		}
	  },
	  {
		"name": "Template expression",
		"scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
		"settings": {
		  "foreground": "#4BE3AC"
		}
	  },
	  {
		"name": "Reset embedded/template expression colors",
		"scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
		"settings": {
		  "foreground": "#FFFFFF"
		}
	  },
	  {
		"name": "YAML key",
		"scope": "entity.name.tag.yaml",
		"settings": {
		  "foreground": "#7868E6"
		}
	  },
	  {
		"name": "JSON key",
		"scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
		"settings": {
		  "foreground": "#7868E6"
		}
	  },
	  {
		"name": "JSON constant",
		"scope": "constant.language.json",
		"settings": {
		  "foreground": "#4BE3AC"
		}
	  },
	  {
		"name": "CSS class",
		"scope": "entity.other.attribute-name.class",
		"settings": {
		  "foreground": "#4BE3AC"
		}
	  },
	  {
		"name": "CSS tag",
		"scope": "source.css entity.name.tag",
		"settings": {
		  "foreground": "#5463FF"
		}
	  },
	  {
		"name": "CSS list",
		"scope": "source.css meta.property-list",
		"settings": {
		  "foreground": "#C8F2EF"
		}
	  },
	  {
		"name": "CSS ID",
		"scope": "entity.other.attribute-name.id",
		"settings": {
		  "foreground": "#C8F2EF"
		}
	  },
	  {
		"name": "CSS name",
		"scope": "source.css support.type.property-name",
		"settings": {
		  "foreground": "#7C83FD"
		}
	  },
	  {
		"name": "CSS value",
		"scope": "source.css support.type.property-value",
		"settings": {
		  "foreground": "#f6ff54"
		}
	  },
	  {
		"name": "HTML tag outer",
		"scope": "meta.tag, punctuation.definition.tag",
		"settings": {
		  "foreground": "#FBF46D"
		}
	  },
	  {
		"name": "HTML tag inner",
		"scope": "entity.name.tag",
		"settings": {
		  "foreground": "#B4FE98"
		}
	  },
	  {
		"name": "HTML tag inner text",
		"scope": "text.html.derivative",
		"settings": {
		  "foreground": "#2bb8ff"
		}
	  },
	  {
		"name": "HTML tag attribute",
		"scope": "entity.other.attribute-name",
		"settings": {
		  "foreground": "#FF7272"
		}
	  },
	  {
		"name": "Markdown heading",
		"scope": "markup.heading",
		"settings": {
		  "foreground": "#4BE3AC"
		}
	  },
	  {
		"name": "Markdown link text",
		"scope": "text.html.markdown meta.link.inline, meta.link.reference",
		"settings": {
		  "foreground": "#A9F1DF"
		}
	  },
	  {
		"name": "Markdown list item",
		"scope": "text.html.markdown beginning.punctuation.definition.list",
		"settings": {
		  "foreground": "#4BE3AC"
		}
	  },
	  {
		"name": "Markdown italic",
		"scope": "markup.italic",
		"settings": {
		  "foreground": "#3EDBF0",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Markdown bold",
		"scope": "markup.bold",
		"settings": {
		  "foreground": "#1CC5DC",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Markdown bold italic",
		"scope": "markup.bold markup.italic, markup.italic markup.bold",
		"settings": {
		  "foreground": "#1CC5DC",
		  "fontStyle": "italic bold"
		}
	  },
	  {
		"name": "Markdown code block",
		"scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
		"settings": {
		  "foreground": "#C8F2EF"
		}
	  },
	  {
		"name": "Markdown inline code",
		"scope": "markup.inline.raw.string.markdown",
		"settings": {
		  "foreground": "#C8F2EF"
		}
	  },
	  {
		"name": "Markdown Punctuation",
		"scope": [
		  "punctuation.definition.heading.markdown",
		  "punctuation.definition.string.begin.markdown",
		  "punctuation.definition.string.end.markdown",
		  "punctuation.definition.quote.begin.markdown",
		  "punctuation.definition.list.begin.markdown",
		  "markup.fenced_code.block.markdown",
		  "meta.separator.markdown"
		],
		"settings": {
		  "foreground": "#356ea0"
		}
	  },
	  {
		"name": "INI property name",
		"scope": "keyword.other.definition.ini",
		"settings": {
		  "foreground": "#ff9746"
		}
	  },
	  {
		"name": "INI section title",
		"scope": "entity.name.section.group-title.ini",
		"settings": {
		  "foreground": "#4BE3AC"
		}
	  },
	  {
		"name": "C# class",
		"scope": "source.cs meta.class.identifier storage.type",
		"settings": {
		  "foreground": "#FE7E6D",
		  "fontStyle": "underline"
		}
	  },
	  {
		"name": "C# class method",
		"scope": "source.cs meta.method.identifier entity.name.function",
		"settings": {
		  "foreground": "#FE7E6D"
		}
	  },
	  {
		"name": "C# function call",
		"scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
		"settings": {
		  "foreground": "#FFE400"
		}
	  },
	  {
		"name": "C# type",
		"scope": "source.cs storage.type",
		"settings": {
		  "foreground": "#FE7E6D"
		}
	  },
	  {
		"name": "C# return type",
		"scope": "source.cs meta.method.return-type",
		"settings": {
		  "foreground": "#FE7E6D"
		}
	  },
	  {
		"name": "C# preprocessor",
		"scope": "source.cs meta.preprocessor",
		"settings": {
		  "foreground": "#1b3951"
		}
	  },
	  {
		"name": "C# namespace",
		"scope": "source.cs entity.name.type.namespace",
		"settings": {
		  "foreground": "#FFFFFF"
		}
	  },
	  {
		"name": "Global settings",
		"settings": {
		//   "background": "#09131b",
		  "foreground": "#FFFFFF"
		}
	  },
	  {
		"name": "Cursive Italic Font",
		"scope": [
		  "modifier",
		  "this",
		  "comment",
		  "storage.modifier.js",
		  "storage.modifier.ts",
		  "storage.modifier.tsx",
		  "entity.other.attribute-name.js",
		  "entity.other.attribute-name.ts",
		  "entity.other.attribute-name.tsx",
		  "entity.other.attribute-name.html",
		  "keyword.control.flow.js"
		],
		"settings": {
		  "fontStyle": "italic"
		}
	  }
	],
	"colors": {
		"focusBorder": "#6EF3D6",
		"foreground": "#FFFFFF",
		"button.background": "#6EF3D6",
		"button.foreground": "#09131b",
		"dropdown.background": "#0e1e2b",
		"input.background": "#0e1e2b",
		"inputOption.activeBorder": "#6EF3D6",
		"list.activeSelectionBackground": "#6EF3D6",
		"list.activeSelectionForeground": "#FFFFFF",
		"list.dropBackground": "#6EF3D680",
		"list.focusBackground": "#6ef3d628",
		"list.focusForeground": "#FFFFFF",
		"list.highlightForeground": "#6EF3D6",
		"list.hoverBackground": "#FFFFFF1a",
		"list.inactiveSelectionBackground": "#FFFFFF33",
		"activityBar.background": "#0c1a25",
		// "activityBar.dropBackground": "#6EF3D680",
		"activityBarBadge.background": "#6EF3D6",
		"activityBarBadge.foreground": "#000000",
		"badge.background": "#6EF3D6",
		"badge.foreground": "#000000",
		"sideBar.background": "#0a1620",
		"sideBarSectionHeader.background": "#0c1a25",
		"editorGroup.dropBackground": "#6EF3D680",
		"editorGroup.focusedEmptyBorder": "#6EF3D6",
		"editorGroupHeader.tabsBackground": "#0a1620",
		"tab.border": "#00000033",
		"tab.activeBorder": "#6EF3D6",
		"tab.lastPinnedBorder": "#6EF3D6",
		"tab.inactiveBackground": "#0c1a25",
		"tab.activeModifiedBorder": "#6EF3D6",
		"tab.inactiveModifiedBorder": "#993c1c",
		"tab.unfocusedActiveModifiedBorder": "#cc5025",
		"tab.unfocusedInactiveModifiedBorder": "#FAEDF0",
		"editor.background": "#09131b",
		"editor.foreground": "#FFFFFF",
		"editorLineNumber.foreground": "#406882",
		"editorLineNumber.activeForeground": "#6EF3D6",
		"editorCursor.foreground": "#6EF3D6",
		"editor.lineHighlightBorder": "#FFFFFF1a",
		"editor.rangeHighlightBackground": "#FFFFFF0d",
		"editorWidget.background": "#0a1620",
		"editorHoverWidget.background": "#0a1620",
		"editorMarkerNavigation.background": "#0a1620",
		"peekView.border": "#6EF3D6",
		"peekViewEditor.background": "#070f15",
		"peekViewResult.background": "#0a1620",
		"peekViewTitle.background": "#09131b",
		"panel.background": "#0a1620",
		"panel.border": "#FFFFFF1a",
		"panelTitle.activeBorder": "#E4FBFF",
		"panelTitle.inactiveForeground": "#FFFFFF80",
		"statusBar.background": "#032D3C",
		"statusBar.debuggingBackground": "#6EF3D6",
		"statusBar.debuggingForeground": "#000000",
		"statusBar.noFolderBackground": "#070f15",
		"statusBarItem.activeBackground": "#6EF3D680",
		"statusBarItem.hoverBackground": "#FFFFFF1a",
		"statusBarItem.remoteBackground": "#6EF3D6",
		"statusBarItem.remoteForeground": "#000000",
		"titleBar.activeBackground": "#032D3C",
		"pickerGroup.border": "#FFFFFF1a",
		"debugToolBar.background": "#0c1a25",
		"selection.background": "#6EF3D6",
		"sash.hoverBorder": "#6EF3D6"
	}
  }
  